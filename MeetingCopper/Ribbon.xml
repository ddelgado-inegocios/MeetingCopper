<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui" >
    <ribbon>
        <tabs >
			<tab idMso="TabAddIns" label="Meeting Tools" insertAfterMso="TabHome"  >
				<group id="ContentGroupMeeting" >
					<button id="textButtonMeeting" label="Nueva Reunión"
					 screentip="Uso General" onAction="NuevaCita"
					 supertip="Generar una Nueva Reunión"
					 getImage="MeetingIcon" size="large"
					  />
				</group>
				<group id="ContentGroupRutina"  >
					<button id="textButtonRutina" label="Task Assignment"
					 screentip="Uso General" onAction="NuevaRutina"
					 supertip="Generar una Nueva Rutina"
					 getImage="RutinaIcon" size="large"
					  />
					
				</group>
				<group id="ContentGroupMinuta"  >
					<button id="textButtonMinuta" label="Nueva Minuta"
					 screentip="Uso General" onAction="NuevaMinuta"
					 supertip="Generar una Nueva Minuta"
					 getImage="MinutaIcon" size="large"
					  />
				</group>
			</tab>
        </tabs>
    </ribbon>
</customUI>